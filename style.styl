nav 
    width: 100%;
    height: 52px;
    border: 1px solid #6a6a6a;
    background-color: #111111;
    display: flex
    align-items: center;
    justify-content: space-around
.w3-container
    max-width: 1541px;
    margin: auto;

.nav-item
    margin-left: 20px

.menu
    color: #a2a1a1;
    font-family: "Roboto Condensed";
    font-size: 11px;
    font-weight: 300;
    list-style none
    display: flex
    

    
.menu-link
      color: #a2a1a1;          
      text-decoration: none
      

.right
    color: white
    list-style none
    display: flex
    align-items: center;
    

.reg-item
    margin-left: 20px        

.b-reg
    text-shadow: 1px 2px 8px rgba(0, 0, 0, 0.75), 0 0 8px rgba(231, 82, 57, 0.75);
    color: gold;
    font-family: "Roboto Condensed";
    font-size: 14px;
    font-weight: 400;
    margin-right: 40px

.fa-bolt
    text-shadow: 1px 2px 1px rgba(0, 0, 0, 0.75), 0 0 8px rgba(57, 139, 231, 0.75);
    color: #76bcde;
    font-size: 14px;
    font-weight: 400;
    line-height: 14.4px;

.lang
    margin-left: 40px    
.flag
    width: 22px;
    height: 22px;
    border: 1px solid #505050;    

.top-wrap
    height: 83px;
    border: 1px solid #6a6a6a;
    background-color: #000000;   
    display: flex
    align-items: center;
    justify-content: space-between

.top-logo
    max-width: 35rem
    height: 70px; 

.top-menu
    margin-right: 5rem
    min-width: 600px

input
    width: 115px;
    height 29px
    border-radius: 3px
    border: #000;
    font-family: "Roboto Condensed - Light Italic";
    font-size: 12px;
    font-weight: 400;
    font-style: italic
    margin: 0 5px

::-webkit-input-placeholder     
    padding: 0 10px
.top-btn
    width: 125px;
    height: 29px;
    box-shadow: 1px 2px 1px rgba(0, 0, 0, 0.75), 0 0 8px 1px rgba(57, 139, 231, 0.75);
    border: 2px solid #81fbff;
    background-color: #2e5fff;
    color: #81fbff;
    font-family: "Roboto Condensed";
    font-size: 14px;
    font-weight: 700;
    margin 0 10px
    cursor: pointer
.logo-wrap
    max-height: 356px;
.bg
    background-image: url('./assets/TOP MENU - LOGO/bg.png')        
.center {
    display: block;
    margin-left: auto;
    margin-right: auto;
    position:relative
    padding: 49px
    z-index: 3
    min-width: 787px;
    min-height: 255px;
    
}
.centered
    min-width: 500px;
    min-height: 225px;
    position: absolute
    top: 50%;
    left: 50%;
    transform: translate(-50%, -125%);
    z-index: 1
.aut
    width: 156px;
    height: 224px;   
    position: absolute
    top: 22%;
    left: 25%; 
.aut-right
    width: 156px;
    height: 224px;   
    position: absolute
    top: 22%;
    right: 25%;     
.spin
    width: 302px;
    height: 124px;     
    position: absolute
    top: 50%;
    left: 50%;
    transform: translate(-50%, -195%);
    z-index: 1
.bottom-menu
    height: 107px;
    box-shadow: 0 5px 1px rgba(0, 0, 0, 0.95), inset 0 3px 3px rgba(0, 10, 72, 0.85), 0 0 16px rgba(32, 90, 255, 0.41), inset 0 0 10px 1px rgba(0, 43, 140, 0.75);
    border: 1px solid #ff96bc;
    background-color: #000000;    

.bot-winner
    margin-top: 1rem
    vertical-align: baseline;

.chip 
    display: inline-block;
    vertical-align: middle;
    padding: 0 25px;
    font-size: 16px;
    color: white
 

.chip p 
    color: #ffffff;
    font-family: "Roboto Condensed";
    font-size: 11px;
    font-weight: 700;       

    
.chip img 
    float: left;
    width: 68px
    height 50px  
    margin-top: 1rem

.chip-text
    margin-top: 5px
    float: right 
    width: 49px;
    height: 38px;
    color: #ffffff;
    font-family: "Roboto Condensed";
.games
    // min-width: 1001px;
    // height: 851px; 
    background-color: black
    margin: auto
    // text-align: center;
.games-wrap
    margin: auto
    padding-left: 8rem  

    
.row
    max-width: 590px;  
    display: flex
    justify-content: flex-start
    margin-left: 5rem
        
.game
    float: left;
    width:180px; 
    height: 122px;
    margin: 18px 
    cursor: pointer
    
.game h5 
    margin: 0 10px   
    color: #e8e9ea;
    font-family: "Roboto Condensed";
    font-size: 13px;
    font-weight: 400;
    line-height: 16px;
    text-transform: uppercase;
.prom
    position: absolute
    max-width: 400px;
    max-height: 308px;
    float  right
    transform: translate(187%, -104%);

footer
    text-align: center 
    display flex
    justify-content: center
    flex-direction: column
    height: 385px;
    border: 1px solid #6a6a6a;
    background-color: #000000;  

footer .menu
    display flex
    justify-content: center
    margin-top: 29px       

.pics
    margin auto
    margin-top: 20px
    width: 969px;
    height: 128px; 
    text-align: center   
.pic 
    margin 40px 20px

      